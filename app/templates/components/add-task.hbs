<div class="layout layout-sm-column">
  {{paper-input class="flex" label="Title Text" type="text" value=title onChange=(action (mut title))}}
  {{paper-input class="flex" label="Description Text" type="text" value=description onChange=(action (mut description))}}
  {{#paper-button primary=true onClick=(action "addTask")}}Add Label{{/paper-button}}
</div>
{{yield}}
